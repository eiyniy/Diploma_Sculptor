{
    "version": "0.2.0",
    "configurations": [
        {
            "program": "${command:cmake.launchTargetPath}",
            "name": "Launch (gdb)",
            "request": "launch",
            "args": [
                // "${workspaceFolder}/resources/models/untitled.obj"
                // "${workspaceFolder}/resources/models/Stone_01_Base.OBJ"
                "${workspaceFolder}/resources/models/rubik.obj"
                // "${workspaceFolder}/resources/models/cat.obj"
                // "${workspaceFolder}/resources/models/woman1.obj"
                // "${workspaceFolder}/resources/models/woman2.obj"
                // "${workspaceFolder}/resources/models/Z3_OBJ.obj"
            ],
            "environment": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "externalConsole": false,
            "internalConsoleOptions": "openOnSessionStart",
            "type": "cppdbg",
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
    ]
}